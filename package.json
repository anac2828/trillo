{
  "name": "yes",
  "version": "1.0.0",
  "description": "Trillo app",
  "default": "index.html",
  "scripts": {
    "watch:sass": "sass sass/main.scss css/styles.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "sass sass/main.scss css/styles.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/styles.comp.css -o css/styles.prefix.css",
    "compress:css": "sass css/styles.prefix.css css/styles.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css",
    "build": "parcel build index.html --dist-dir ./dist"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "npm-run-all": "^4.1.5",
    "parcel": "^2.8.3",
    "postcss-cli": "^10.1.0",
    "sass": "^1.90.0"
  }
}
